# 데이터베이스 연결 정보
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public"
# CSRF 보호를 위한 비밀 키
CSRF_SECRET="change-me-to-a-secure-random-value"
# 관리자 페이지 접근 토큰
ADMIN_TOKEN="set-a-strong-admin-token"
# 허용할 도메인 목록(쉼표로 구분)
ALLOWED_ORIGINS="https://yourdomain.com,https://www.yourdomain.com"
# 분당 허용 요청 수
RATE_LIMIT_MAX="120"
# 클라이언트에서 사용할 소켓 주소 (기본값은 동일 도메인)
# NEXT_PUBLIC_SOCKET_URL="https://yourdomain.com"
